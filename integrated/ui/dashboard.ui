&lt;span size='small'&gt;View member analytics&lt;/span&gt;</property>
                                <property name="use_markup">True</property>
                                <property name="justify">GTK_JUSTIFY_CENTER</property>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
              </packing>
            </child>

            <!-- Coach Management Section -->
            <child>
              <object class="GtkFrame" id="frame_coach_section">
                <property name="visible">True</property>
                <property name="label_xalign">0.5</property>
                <property name="shadow_type">GTK_SHADOW_ETCHED_IN</property>
                <child type="label">
                  <object class="GtkLabel" id="label_frame_coaches">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">&lt;span size='large' weight='bold'&gt;üë®‚Äçüè´ COACH MANAGEMENT&lt;/span&gt;</property>
