                <property name="visible">True</property>
                <property name="spacing">5</property>
                <property name="border_width">20</property>

                <child>
                  <object class="GtkLabel" id="dashboard_header_title">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">&lt;span size='xx-large' weight='bold'&gt;üèãÔ∏è GYM MANAGEMENT SYSTEM&lt;/span&gt;</property>
                    <property name="use_markup">True</property>
                    <property name="xalign">0.5</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                  </packing>
                </child>

                <child>
                  <object class="GtkLabel" id="dashboard_subtitle">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">&lt;span size='large'&gt;Professional Management Dashboard&lt;/span&gt;</property>
                    <property name="use_markup">True</property>
                    <property name="xalign">0.5</property>
                  </object>
                  <packing>
